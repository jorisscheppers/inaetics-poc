default flatcar
prompt 1
timeout 15

display boot.msg

label flatcar
  menu default
  kernel flatcar_production/flatcar_production_pxe.vmlinuz
  initrd flatcar_production/flatcar_production_pxe_image.cpio.gz
  append flatcar.first_boot=1 ignition.config.url=http://schepds.local/ignition-configs/c0-3f-d5-67-01-ab.ign