default flatcar
prompt 1
timeout 15

display boot.msg

label flatcar
  menu default
  kernel flatcar_production/flatcar_production_pxe.vmlinuz
  initrd flatcar_production/flatcar_production_pxe_image.cpio.gz
  append flatcar.first_boot=1 ignition.config.url=https://raw.githubusercontent.com/jorisscheppers/inaetics-poc/main/ignition-configs/c0-3f-d5-67-02-9f-pxe.ign