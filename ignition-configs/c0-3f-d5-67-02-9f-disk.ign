{
  "ignition": {
    "config": {},
    "security": {
      "tls": {}
    },
    "timeouts": {},
    "version": "2.3.0"
  },
  "networkd": {},
  "passwd": {
    "users": [
      {
        "groups": [
          "sudo",
          "docker"
        ],
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa "
        ]
      }
    ]
  },
  "storage": {
    "files": [
      {
        "filesystem": "root",
        "path": "/etc/hostname",
        "contents": {
          "source": "data:,node2.cluster.local",
          "verification": {}
        },
        "mode": 420
      },
      {
        "filesystem": "root",
        "path": "/opt/scripts/tftp/tftp.bash",
        "contents": {
          "source": "https://raw.githubusercontent.com/jorisscheppers/inaetics-poc/main/scripts/tftp/tftp.bash",
          "verification": {}
        },
        "mode": 555
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Init tftp\nRequires=network-online.target\nAfter=network-online.target\n[Service]\nType=oneshot\nExecStart=/opt/scripts/tftp/tftp.bash\nRemainAfterExit=true\nStandardOutput=journal\n[Install]\nWantedBy=multi-user.target",
        "enabled": true,
        "name": "installer.service"
      }
    ]
  }
}
